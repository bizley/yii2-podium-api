{
  "name": "bizley/podium-api",
  "type": "yii2-extension",
  "description": "Yii 2 Forum API.",
  "keywords": ["forum", "yii2", "extension", "podium", "module", "api", "bizley"],
  "license": "Apache License 2.0",
  "support": {
    "issues": "https://github.com/bizley/yii2-podium-api/issues",
    "wiki": "https://github.com/bizley/yii2-podium-api/wiki",
    "source": "https://github.com/bizley/yii2-podium-api"
  },
  "authors": [
    {
      "name": "Pawel Bizley Brzozowski",
      "email": "pawel@positive.codes"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": ">=2.0.15.1 <2.1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "bizley\\podium\\api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bizley\\podium\\tests\\": "tests/"
    }
